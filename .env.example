# Printavo API settings
PRINTAVO_API_URL=https://www.printavo.com/api/v2
PRINTAVO_EMAIL=your_printavo_email_here
PRINTAVO_TOKEN=your_printavo_token_here
NEXT_PUBLIC_PRINTAVO_API_URL=https://www.printavo.com/api/v2
NEXT_PUBLIC_PRINTAVO_EMAIL=your_printavo_email_here
NEXT_PUBLIC_PRINTAVO_TOKEN=your_printavo_token_here
# Set to "true" to use the Printavo MCP server or "false" to use direct API calls
USE_PRINTAVO_MCP=true

# IMPORTANT: Do not change the API URL format - it must be exactly as shown above
# The application will automatically add the https:// protocol when making requests 

# OpenAI API settings for voice and chat features
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
OPENAI_VOICE_MODEL=gpt-4o-realtime-preview
OPENAI_TRANSCRIPTION_MODEL=whisper-1

# SanMar API credentials
SANMAR_USERNAME=your_sanmar_username_here
SANMAR_PASSWORD=your_sanmar_password_here
SANMAR_CUSTOMER_NUMBER=your_sanmar_customer_number_here
SANMAR_CUSTOMER_IDENTIFIER=your_sanmar_customer_identifier_here

# SanMar FTP credentials
SANMAR_FTP_HOST=ftp.sanmar.com
SANMAR_FTP_USERNAME=your_sanmar_ftp_username_here
SANMAR_FTP_PASSWORD=your_sanmar_ftp_password_here
SANMAR_FTP_PORT=22

# OpenAI Assistants API configuration
USE_OPENAI_ASSISTANTS=true
PRINTAVO_ASSISTANT_ID=your_printavo_assistant_id_here

# Supabase configuration for agent storage
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Python Agent Service configuration
USE_PYTHON_AGENT=false
PYTHON_AGENT_URL=http://localhost:8000

